doctype html
html(lang="ja")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(href="https://fonts.googleapis.com/css?family=Noto+Serif+JP:400,600&amp;subset=japanese",rel="stylesheet")
    link(rel="stylesheet", href="./../styles/ikanoosushi/ikanoosushi.css")
    link(rel="icon" type="image/x-icon" href="./../images/favicon.ico")
    title いかのおすし | kosen14s #dojin
    meta(property="og:locale", content="ja_JP")
    meta(property="og:site_name", content="kosen14s")
    meta(property="og:type", content="website")
    meta(property="og:url", content="https://kosen14s.github.io/ikanoosushi")
  body
    header
      .home-link
        a(href="./../"): img.logo14s(src="./../images/techbookfest6/14s_dojin_logo.svg", alt="kosen14s #dojin")
      .title: img.title-img(src="./../images/techbookfest6/ikanoosushi_logo.svg", alt="いかのおすし")
      .scroll
        a(href="#techbook"): img.scroll-img(src="./../images/techbookfest6/scroll_down.svg", alt="scroll down!")
      .detail
        .pages 90ページ
        .variation
          .name 物理本
          .value ¥800
        .variation
          .name PDF版
          .value ¥600
    main
      section#techbook
      section#about
      section#contents
      section#products
    footer