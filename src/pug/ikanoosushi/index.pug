      section#contents
        .section-top
          .section-BG.BG2
          .section-title: .text コンテンツ
      section#products
        .section-top
          .section-BG.BG3
          .section-title: .text その他の販売物
          .sns
            a.share#twitter(href="http://")
              img(src="./../images/techbookfest6/share_tw.svg")
              .share-text Twitterでつぶやく
            a.share#facebook(href="http://")
              img(src="./../images/techbookfest6/share_fb.svg")
              .share-text Facebookでシェア
    footer